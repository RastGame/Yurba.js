{
  "name": "Yurba.js Documentation",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/yurba.js",
    "packages/rest",
    "packages/ws",
    "packages/types"
  ],
  "out": "apps/docs",
  "theme": "default",
  "includeVersion": true,
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": false,
  "hideGenerator": true,
  "readme": "README.md",
  "categorizeByGroup": true,
  "sort": ["alphabetical"],
  "gitRevision": "main",
  "sourceLinkTemplate": "https://github.com/RastGame/Yurba.js/blob/{gitRevision}/{path}#L{line}",
  "cleanOutputDir": true,
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "skipErrorChecking": true
}