{
  "name": "Yurba.js Documentation",
  "entryPoints": [
    "packages/yurba.js",
    "packages/rest",
    "packages/ws",
    "packages/types"
  ],
  "entryPointStrategy": "packages",
  "out": "docs",
  "theme": "default",
  "includeVersion": false,
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeInternal": true,
  "hideGenerator": false,
  "readme": "README.md",
  "categorizeByGroup": true,
  "sort": ["source-order"],
  "searchInComments": true,
  "customCss": "./docs-theme/custom.css",
  "gitRevision": "main",
  "gitRemote": "origin",
  "cleanOutputDir": true,
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}