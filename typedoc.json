{
  "name": "Yurba.js",
  "entryPoints": [
    "packages/yurba.js/src/index.ts"
  ],
  "out": "docs",
  "theme": "default",
  "includeVersion": false,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "readme": "none",
  "categorizeByGroup": true,
  "categoryOrder": [
    "Classes",
    "Interfaces", 
    "Types",
    "Functions",
    "*"
  ],
  "sort": ["alphabetical"],
  "searchInComments": true,
  "searchInDocuments": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "kindSortOrder": [
    "Project",
    "Module", 
    "Class",
    "Interface",
    "TypeAlias",
    "Function",
    "Variable",
    "Property",
    "Method",
    "Constructor",
    "Accessor"
  ],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "customCss": "./docs-theme/discord-style.css",
  "gitRevision": "main",
  "sourceLinkTemplate": "https://github.com/yurbajs/yurba.js/blob/{gitRevision}/{path}#L{line}",
  "titleLink": "https://yurba.js.org",
  "cleanOutputDir": true,
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}