{
  "$schema": "https://typedoc.org/schema.json",
  "name": "Yurba.js",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/yurba.js",
    "packages/rest",
    "packages/ws",
    "packages/types"
  ],
  "out": "docs",
  "theme": "default",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "readme": "docs-README.md",
  "categorizeByGroup": true,
  "sort": ["alphabetical"],
  "searchInComments": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "customCss": "./docs-theme/modern-style.css",
  "gitRevision": "main",
  "sourceLinkTemplate": "https://github.com/RastGame/Yurba.js/blob/{gitRevision}/{path}#L{line}",
  "titleLink": "https://yurba.js.org",
  "cleanOutputDir": true,
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "plugin": ["typedoc-plugin-mdn-links"],
  "sidebarLinks": {
    "GitHub": "https://github.com/RastGame/Yurba.js",
    "NPM": "https://www.npmjs.com/package/yurba.js",
    "Yurba.one": "https://yurba.one"
  },
  "favicon": "./assets/logo.png"
}