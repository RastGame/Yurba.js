{
  "name": "yurba.js",
  "version": "0.0.1",
  "description": "Yurba Bot Library",
  "main": "./lib/index.js",
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && tsc",
    "watch": "tsc --watch",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node ./lib/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "lintjs": "eslint lib --ext .js",
    "lintjs:fix": "eslint lib --ext .js --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "docs": "typedoc src/index.ts src/types/index.ts",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rastgame/yurba.js.git"
  },
  "keywords": [
    "yurba",
    "api",
    "bot",
    "node",
    "yurbaapp",
    "yurba.one"
  ],
  "author": "rastgame",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rastgame/yurba.js/issues"
  },
  "homepage": "https://github.com/rastgame/yurba.js#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.31.1"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ]
}
